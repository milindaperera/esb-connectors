<soapenv:Envelope xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="urn:Magento">
   <soapenv:Header/>
   <soapenv:Body>
      <urn:shoppingCartPaymentMethod soapenv:encodingStyle="http://schemas.xmlsoap.org/soap/encoding/">
         <sessionId xsi:type="xsd:string">%s(sessionId)</sessionId>
         <quoteId xsi:type="xsd:int">%s(createInvoiceOptionalQuoteId)</quoteId>
         <method xsi:type="urn:shoppingCartPaymentMethodEntity">
            <method>%s(paymentDataMethod)</method>
         </method>
         <storeId xsi:type="xsd:string">%s(storeId)</storeId>
      </urn:shoppingCartPaymentMethod>
   </soapenv:Body>
</soapenv:Envelope>