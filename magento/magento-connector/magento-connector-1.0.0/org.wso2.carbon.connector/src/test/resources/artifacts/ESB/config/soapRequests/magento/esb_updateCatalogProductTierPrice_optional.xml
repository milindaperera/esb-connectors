					<customer_group_id>2</customer_group_id>
					<website>all</website>
					<qty>12</qty>
					<price>2000</price>
				</updatecatalogproducttierprice>
				<updatecatalogproducttierprice>
					<customer_group_id>1</customer_group_id>
					<website>all</website>
					<qty>15</qty>
					<price>5000</price>
				</updatecatalogproducttierprice>
		</urn:tierPrices>
		<urn:method>updateCatalogProductTierPrice</urn:method>
	</root>
  </soapenv:Body>
</soapenv:Envelope>