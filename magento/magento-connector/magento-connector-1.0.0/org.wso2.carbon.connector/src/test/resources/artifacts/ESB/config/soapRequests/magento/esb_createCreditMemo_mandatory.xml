<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="wso2.connector.magento">
	<soapenv:Header></soapenv:Header>
	<soapenv:Body>
		<root>
			<urn:apiUser>%s(apiUser)</urn:apiUser>
			<urn:apiKey>%s(apiKey)</urn:apiKey>
			<urn:apiUrl>%s(apiUrl)</urn:apiUrl>
			<urn:orderIncrementId>%s(orderIncrementId)</urn:orderIncrementId>
			<urn:creditmemoData>
				<values>
					<shipping_amount>0</shipping_amount>
					<adjustment_positive>0</adjustment_positive>
					<adjustment_negative>1</adjustment_negative>
				</values>
			</urn:creditmemoData>
			<!-- Send Connector method name to be called -->
			<urn:method>createCreditMemo</urn:method>
		</root>
	</soapenv:Body>
</soapenv:Envelope>