<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope 
	xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
	xmlns:urn="wso2.connector.magento">
  <soapenv:Header></soapenv:Header>
  <soapenv:Body>
    <root>
		<urn:apiUser>%s(apiUser)</urn:apiUser>
		<urn:apiKey>%s(apiKey)</urn:apiKey>
		<urn:apiUrl>%s(apiUrl)</urn:apiUrl>
		<urn:quoteId>%s(quoteId)</urn:quoteId>
		<urn:products>
			<shoppingCartProductEntity>
				<product_id>%s(productIdMandatory)</product_id>
			</shoppingCartProductEntity>
			<shoppingCartProductEntity>
				<product_id>%s(productIdOptional)</product_id>
			</shoppingCartProductEntity>
		</urn:products>
		<!-- Send Connector method name to be called -->
		<urn:method>addCartProduct</urn:method>
	</root>
  </soapenv:Body>
</soapenv:Envelope>