<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="wso2.connector.magento">
  <soapenv:Header></soapenv:Header>
  <soapenv:Body>
    <root>
		<urn:apiUser>%s(apiUser)</urn:apiUser>
		<urn:apiKey>%s(apiKey)</urn:apiKey>
		<urn:apiUrl>%s(apiUrl)</urn:apiUrl>
		<urn:storeId>%s(storeId)</urn:storeId>
		<urn:quoteId>%s(moveProductFromQuoteToCartQuoteId)</urn:quoteId>
		<urn:products>
			<shoppingCartProductEntity>
				<product_id>%s(productIdMandatory)</product_id>
				<qty>1</qty>
			</shoppingCartProductEntity>
		</urn:products>
		<!-- Send Connector method name to be called -->
		<urn:method>moveProductFromQuoteToCart</urn:method>
	</root>
  </soapenv:Body>
</soapenv:Envelope>