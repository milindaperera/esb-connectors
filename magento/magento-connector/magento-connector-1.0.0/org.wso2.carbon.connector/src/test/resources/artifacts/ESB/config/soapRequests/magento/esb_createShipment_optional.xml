<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="wso2.connector.magento">
  <soapenv:Header></soapenv:Header>
  <soapenv:Body>
  	<root>
		<urn:method>createShipment</urn:method>
  		<urn:apiUser>%s(apiUser)</urn:apiUser>
		<urn:apiKey>%s(apiKey)</urn:apiKey>
		<urn:apiUrl>%s(apiUrl)</urn:apiUrl>
	    <urn:orderIncrementId>%s(createInvoiceOptOrderIncId)</urn:orderIncrementId>
	    <urn:itemsQty>
	    	<orderItemQty>
	    		<order_item_id></order_item_id>
	    		<qty></qty>
	    	</orderItemQty>
	    </urn:itemsQty>
	    <urn:comment>%s(comment)</urn:comment>
	    <urn:email></urn:email>
	    <urn:includeComment></urn:includeComment>
	</root>
  </soapenv:Body>
</soapenv:Envelope>