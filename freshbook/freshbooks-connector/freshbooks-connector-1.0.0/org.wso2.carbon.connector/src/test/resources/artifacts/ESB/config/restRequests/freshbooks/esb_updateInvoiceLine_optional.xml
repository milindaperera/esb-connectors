<updateInvoiceLine>
	<arbitraryPassword>%s(arbitraryPassword)</arbitraryPassword>
	<apiUrl>%s(apiUrl)</apiUrl>
	<authenticationToken>%s(authenticationToken)</authenticationToken>
	<invoiceId>%s(invoiceIdOptional)</invoiceId>
	<lines>
		<line>
			<line_id>%s(invoiceLineIdOptional)</line_id>
			<name>updatedLine</name>
			<description>%s(description)</description>
			<unit_cost>1000</unit_cost>
			<quantity>40</quantity>
			<tax1_name>VAT2</tax1_name>
			<tax2_name>VAT2D</tax2_name>
			<tax1_percent>12</tax1_percent>
			<tax2_percent>23</tax2_percent>
			<compound_tax>%s(compoundTax)</compound_tax>
			<type>Item</type>
		</line>
	</lines>
</updateInvoiceLine>
