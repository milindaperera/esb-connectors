<component name="soundcloud_groups" type="synapse/template">
    <subComponents>
        <component name="getGroups">
            <file>getGroups.xml</file>
            <description>Get all groups</description>
        </component>
        <component name="getGroupById">
            <file>getGroupById.xml</file>
            <description>Get soundcloud group information by group Id</description>
        </component>
        <component name="getModeratorsOfGroupById">
            <file>getModeratorsOfGroupById.xml</file>
            <description>Get list of users who moderate the group</description>
        </component>
        <component name="getMembersOfGroupById">
            <file>getMembersOfGroupById.xml</file>
            <description>Get list of users who joined the group</description>
        </component>
        <component name="getContributorsOfGroupById">
            <file>getContributorsOfGroupById.xml</file>
            <description>Get list of users who contributed a track to the group</description>
        </component>
        <component name="getUsersOfGroupById">
            <file>getUsersOfGroupById.xml</file>
            <description>Get list of users who contributed to, joined or moderate the group</description>
        </component>
        <component name="getTracksOfGroupById">
            <file>getTracksOfGroupById.xml</file>
            <description>Get list of contributed and approved tracks</description>
        </component>
    </subComponents>
</component>