<component name="soundcloud_resolve" type="synapse/template">

    <subComponents>
        <component name="resolveAllTracksOfUser">
            <file>resolveAllTracksOfUser.xml</file>
            <description>Resolve all tracks of user by permalink of the user</description>
        </component>
        <component name="resolveTrack">
            <file>resolveTrack.xml</file>
            <description>'Resolve one track by permalink of the user and permalink of the track</description>
        </component>
        <component name="resolveUser">
            <file>resolveUser.xml</file>
            <description>Resolve user by permalink of the user</description>
        </component>
        <component name="resolveAllGroupsOfUser">
            <file>resolveAllGroupsOfUser.xml</file>
            <description>Resolve all groups of user by permalink of the user</description>
        </component>
        <component name="resolveApp">
            <file>resolveApp.xml</file>
            <description>Resolve app by app id</description>
        </component>
    </subComponents>
</component>